{
  "name": "turbo-2nd",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "db:generate": "cd packages/db && bunx prisma generate && cd ../..",
    "build:web": "cd apps/web && bun run build && cd ../..",
    "start:web": "cd apps/web && bun run start",
    "start:backend": "cd apps/backend && bun run src/index.ts",
    "start:ws": "cd apps/ws && bun run src/index.ts"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.6.1",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.3.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
